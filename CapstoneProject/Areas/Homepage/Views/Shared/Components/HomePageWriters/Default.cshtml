@model List<GetWritersImageQueryResult>

<div class="axil-post-list-area post-listview-visible-color axil-section-gap bg-color-white">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-xl-12 mt_md--40 mt_sm--40">
                <div class="sidebar-inner">
                    <div class="axil-single-widget widget widget_instagram mb--30">
                        <h5 class="widget-title">Writers</h5>
                        <ul class="instagram-post-list-wrapper">
                            @foreach (var item in Model)
                            {
                                <li class="instagram-post-list">
                                    <a href="#">
                                        <img style="height:100px; width:200px;" src="~/IMAGES/UserImages/@item.ImageUrl" alt="Instagram Images">
                                    </a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>